    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "font-awesome": "^4.7.0",
    "google-fonts-webpack-plugin": "^0.4.4",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier-standard": "^16.1.0",
