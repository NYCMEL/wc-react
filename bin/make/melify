#!/Melify/bin/mac/tclkit

if {$argc == 0} {
    puts ""
    puts "---------------------------------"
    puts "Commands:"

    foreach i [glob /Melify/bin/make/make.*] {
	puts "\t[file tail $i]"
    }
    puts "---------------------------------"
    puts ""
    exit
}

exec /Melify/bin/uni /Melify/bin/make/$argv
